switch(@Manager)
{
case "ProbeWatch":
  switch(@Summary)
  {
    case "Going Down ...":
      @AlertGroup = "ProbeUpDown"
      @Type = 1
    case "Running ...":
      @AlertGroup = "ProbeUpDown"
      @Type = 2
    default:
  }
  @Summary = @Agent + " on " + @Node + " " + @Summary
  @AlertKey = @Agent
#######################################################################
# 
# The Defaults start here for main rules section (non ProbeWatch alerts).
# !! This IS where management system event processing starts
#
default:
  @Class      = 400
  @NodeAlias  = $IPAddress
  @Summary    = "??: " + $enterprise + " " + $generic-trap + " " + $specific-trap
  @Manager    = "RAD"
  @Severity   = "1"
  @Type       = "1"

  if ( !exists($Node) ) {
    $Node = $PeerAddress
  }

  $v1trap = 1

  # An SNMP v2 trap?
  if (exists($notify)) {
    # Uncomment the line below to use V2 trap case statements
    # $v1trap = 0

    # Remove the artifacts of ucd_snmp library being 'helpful'
    # We'll only remove the first .iso., everything else should be
    # just plain numbers. With the mttrapd probe, this has to be
    # done by specifying /dev/null as the mib file.
    if ( regmatch( $notify, "\.iso\." ) ) {
        $notify = ".1." + extract( $notify, "\.iso\.(.*)" )
    }
    $switch-val = $notify # This is only significant if $v1trap is 0

    # Convert the trap into V1 form just in case
    if ( nmatch( $notify, ".1.3.6.1.6.3.1.1.5." ) ) {
      # This is a generic V2 trap
      $enterprise = ".1.3.6.1.6.3.1.1.5"
      $generic-trap = int(extract( $notify, ".*([^\.]*)$" )) - 1
    } else {
      # A V2 specific trap
      $enterprise = extract($notify, "[^\.]*(.*)\.[0-9]\.[^\.]*$")
      $specific-trap = extract($notify, ".*\.[0-9]\.([^\.]*)$")
      $generic-trap = "6"
    }
  }

  # Insert OID stripping rules here.
  # include "oidstripping.rules"

  if ( int($v1trap)==1 ) {
    if ( match($generic-trap, "6") ) {
      $switch-val = $enterprise + "|6|" + $specific-trap
    } else {
      $switch-val = $enterprise + "|" + $generic-trap
    }
  }

  # If this is an internal OpenView or NV6K trap, then take the value
  # for @Node from $2 rather than the normal $Node, which for these
  # traps will equate to the OV or NV6K management system name
  #
  # This replaces the older property called InternalTraps
  #
  if ( nmatch ($enterprise,".1.3.6.1.4.1.2.6.3") OR
       nmatch ($enterprise,".1.3.6.1.4.1.2.6.3.1") OR
       nmatch ($enterprise,".1.3.6.1.4.1.11.2.17.1") )
  {
    $Node = $2
  }
  @Node = $Node

  @Agent = $enterprise
  $enterprise-name = $enterprise

  if ( nmatch ($enterprise,".1.3.6.1.4.1.11.2.17.1") )
  {
    include "$NCHOME/custom/probes/rules/nnm6/include/rad.hpov.include.rules" 
  }
  # JL 24072007 deal with system alarms
  if ( nmatch ($enterprise,".1.3.6.1.4.1.164.6.1") )
  {
    include "$NCHOME/custom/probes/rules/nnm6/include/rad.system.include.rules" 
  }



