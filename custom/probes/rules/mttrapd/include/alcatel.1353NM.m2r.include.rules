### section addded by Toby
## to account for friendly name format not seen in osos interface rulesfile.#
## sample FELYMSE01/r01s1/board#04/Regenerator#1

    if(regmatch($friendlyName, ".*/.*/.*/.*"))
    {
        @Node = extract($friendlyName, "^([^\/]*)/.*/.*/.*")
        if(regmatch($friendlyName, ".*/r.*"))
        {
            $MOI = "Rack: " + extract($friendlyName, ".*/r([0-9]+).*")
        }
### section altered by Nick Evans
## for WP160 Patch 4.38 22062011
        if(regmatch($friendlyName, ".*sr[0-9]+[s,l,b]+.*"))
        {
            if(!match($MOI, ""))
            {
                $MOI = $MOI + ", "
            }
            $MOI = $MOI + "Sub-Rack: " + extract($friendlyName, ".*sr([0-9]+)[s,l,b].*") + ", Slot: " + extract($friendlyName, ".*[s,l,b]+([0-9]+)/.*")
        }
	else if(regmatch($friendlyName, ".*/.*s.*/.*/.*"))
	{
	     if(!match($MOI, ""))
            {
                $MOI = $MOI + ", "
            }
		$MOI = $MOI + "Slot: " + extract($friendlyName, ".*/.*s(.*)/.*/.*") + ", " +  extract($friendlyName, ".*/.*s.*/(.*)/.*")

        	$MO = extract($friendlyName, ".*/.*/.*/(.*)")
	}

        if(!match($MO, ""))
        {
            if(!match($MOI, ""))
            {
                $MOI = $MOI + ", "
            }
         #   $MOI = $MOI + "Managed Obj.: " + $MO
       #Summary Field Changed as part of 10G Project      
            $MOI = $MOI + "" + $MO 
        }
    }
### end of section added by Toby

    else if(regmatch($friendlyName, ".*/.*/.*"))
    {
        @Node = extract($friendlyName, "^([^\/]*)/.*/.*")
        if(regmatch($friendlyName, ".*/r.*"))
        {
            $MOI = "Rack: " + extract($friendlyName, ".*/r([0-9]+).*")
        }
        if(regmatch($friendlyName, ".*sr[0-9]+[s,l,b]+.*"))
        {
            if(!match($MOI, ""))
            {
                $MOI = $MOI + ", "
            }
            $MOI = $MOI + "Sub-Rack: " + extract($friendlyName, ".*sr([0-9]+)[s,l,b]+.*") + ", Slot: " + extract($friendlyName, ".*[s,l,b]+([0-9a-z]+)/.*")
        }
        $MO = extract($friendlyName, ".*/.*/(.*)")
        if(!match($MO, ""))
        {
            if(!match($MOI, ""))
            {
                $MOI = $MOI + ", "
            }
           # $MOI = $MOI + "Managed Obj.: " + $MO
           $MOI = $MOI + "" + $MO
        }
    }
    else if (regmatch($friendlyName, ".*/.*"))
    {
        @Node = extract($friendlyName, "^(.*)/.*")
        $MO = extract($friendlyName, ".*/(.*)")
        if(!match($MO, ""))
        {
        #    $MOI = "Managed Obj.: " + $MO
           $MOI = $MOI + "" + $MO
 
        }
    }
    else if(exists($friendlyName))
    {
       # @Node = $friendlyName
    } else
    {
### added by Toby as some alarms have no friendly name ?????
	@Node = $IPaddress
    }
    if(match($eventType, "Node Isolation")){
	@Node = $friendlyName
    }
    if(match($probableCause, "Node Isolation") && match(@Type, "2")){
        @Node = $friendlyName
    }
    
    @AlertGroup = "NE Alarm" 
#Changed Manger field under Patch 3.105
    @Manager = "1353NM"
    @AlertKey = "Alarm ID: " + $currentAlarmId
    switch($perceivedSeverity)
    {
        case "critical":
            @Severity = 5
        case "major":
            @Severity = 4
        case "minor":
            @Severity = 3
        case "warning":
            @Severity = 2
        case "indeterminate":
            @Severity = 2
        case "cleared":
            @Severity = 1
        default:
            @Severity = 2
    }
    switch($notificationType)
    {
        case "alarmRaise"|"alarmList":
            switch($eventType)
            {
                case "communicationsAlarm":
                    @X733EventType = 1
                    @Summary = "Communications Alarm"
                case "environmentAlarm":
                    @X733EventType = 5
                    @Summary = "Environmental Alarm"
                case "equipmentAlarm":
                    @X733EventType = 4
                    @Summary = "Equipment Alarm"
                case "processingErrorAlarm":
                    @X733EventType = 3
                    @Summary = "Processing Error"
                case "qualityofServiceAlarm":
                    @X733EventType = 2
                    @Summary = "Quality of Service Alarm"
                default:
                    @X733EventType = 0
                    @Summary = "Unknown Event Type"
            }
            @Type = 1
        case "alarmClear":
            @Summary = "NE Alarm Cleared"
            @Severity = 1
            @Type = 2
        case "alarmAck":
            discard
        default:
    }
    if(!match($probableCause, ""))
    {
      #  @Summary = @Summary + "," + $probableCause
        @Summary = @Summary + $probableCause 
    }
#    if(!match($specificProblems, ""))
#    {
#        @Summary = @Summary + ", " + $specificProblems
#    }

    if(match($stage, "2nd stage input"))
    {
        @Summary = @Summary + ", " + $stage
    }
   
   if(match($stage, "1st stage output"))
    {
        @Summary = @Summary + ", " + $stage
    }
   if(match($stage, "2nd stage output"))
    {
        @Summary = @Summary + ", " + $stage
    }
 

    if(!match($additionalText, "")&&!match($additionalText, "specificProblems"))
    {
        @Summary = @Summary + ", " + $additionalText
    }
    if(!match($MOI, ""))
    {
        @Summary = @Summary + "  ( " + $MOI + " )"
    }

### sometimes probable cause not set in clear alarm???
        if(match(@Summary,"alarmClear"))
        {
                @Summary = @Summary + ", " + @AlarmClass
        }

