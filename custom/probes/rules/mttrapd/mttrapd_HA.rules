				# 20120222	Chris Janes	Rulrs added for IPT
				include "/opt/IBM/tivoli/netcool/custom/probes/rules/mttrapd/include/IPTNEMMANAGEMENT.include.rules"
				# 20120222	End of Change
			
				###################################################################
				# End of rules file Includes
				###################################################################

			default:

				$EventID = $enterprise + "|" + $generic-trap + "|" + $specific-trap

				switch($EventID)
				{
					case "dummy case statement": ### This will prevent syntax errors in case no includes are added below.

					###########################################################
